{
  "name": "devhouse",
  "version": "1.0.0",
  "description": "",
  "main": ".eslintrc.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build-tailwind": "npx tailwindcss-cli@latest build ./src/public/css/tailwind.css -o ./dist/public/css/tailwind.css",
    "build-ts": "tsc",
    "build": "npm run build-tailwind && npm run build-ts && npm run lint && npm run copy-static-assets",
    "copy-static-assets": "ts-node script.ts",
    "debug": "npm run build && npm run watch-debug",
    "dev2": "cross-env NODE_ENV=development nodemon",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "prisma:init": "prisma init",
    "prisma:migrate": "prisma migrate dev --preview-feature",
    "prisma:generate": "prisma generate",
    "prisma:seed": "prisma db seed --preview-feature",
    "serve-debug": "nodemon --inspect dist/server.js",
    "serve": "node dist/server.js",
    "start1": "npm run serve",
    "start": "node dist/server.js",
    "postinstall": "cross-env NODE_ENV=production && npm run build",
    "dev": "cross-env NODE_ENV=production && npm run build && node dist/server.js",
    "test": "jest --forceExit --coverage --verbose",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/server.js",
    "watch-sass": "sass --watch src/public/css/main.scss dist/public/css/main.css",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "prisma": {
    "schema": "src/database/prisma/schema.prisma"
  },
